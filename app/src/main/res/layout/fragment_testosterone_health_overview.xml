<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="edu.mills.freeingourselves.TestosteroneHealthOverviewFragment"
    android:background="#ffffff">
    <Button
        android:text="@string/holistic_care"
        android:id="@+id/holisticCareButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"/>

    <WebView
        android:id="@+id/tWebView"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:layout_above="@+id/holisticCareButton"/>
</RelativeLayout>